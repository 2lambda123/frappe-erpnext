 "css": ".body_one{S\n    height:100%;\n    width:100%;\n}\n.footer{\n    position: absolute;\n    bottom: 0;\n    margin-left:10%;\n}\n#div_tab{\n    width:100%;\n    color:black;\n    margin-top:10px;\n    font-size:12px;\n}\n.body_two{\n    font-size:8px; \n    color:black; \n    margin-top:20px;\n    text-align:left;\n    line-height: 1.0;\n}\n.table1{\n\tmax-width: 100%;\n\tmargin: 0 auto;\n    width: 90%;\n    text-align:justify;\n    font-size:small;\n}\nth{\n    background:#E6E6E6;\n    color:black;\n    font-size:8px;\n}\n#col{\n    border:1px solid #CBCBCB;\n\tborder-collapse: collapse;\n\tfont-size:9px;\n\ttext-align: center;\n}\n#in_word{\n    height:100%;\n    width:100%;\n    color:black;\n    padding-top:20px;\n}",
