 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">GOODS TRANSFER NOTE</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">\r\n            {%- if doc.items[0].cost_center -%}\r\n                {{ doc.items[0].cost_center }}\r\n            {%- endif -%}\r\n        </p>\r\n        <p style=\"font-size:14px;text-align:center;\">\r\n            <strong>\r\n                GTN No:\r\n            </strong>\r\n            {{doc.name}}\r\n        </p>\r\n    </div>\r\n   \r\n   <div id=\"div_tab\">\r\n        <p style=\"float:right\"><strong>GTN Date:</strong>{{doc.posting_date}}</p>\r\n    </div>\r\n    <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Item Code</strong></th>\r\n                <th id=\"col\"><strong>Item Name</strong></th>\r\n                <th id=\"col\"><strong>Transporter Name</strong></th>\r\n                <th id=\"col\"><strong>Tyre Weight(MT)</strong></th>\r\n                <th id=\"col\"><strong>Gross Weight(MT)</strong></th>\r\n                <th id=\"col\"><strong>Weight Slip No.</strong></th>\r\n                <th id=\"col\"><strong>POL Slip No</strong></th>\r\n                <th id=\"col\"><strong>Vehicle No.</strong></th>\r\n                <th id=\"col\"><strong>Vehicle Type</strong></th>\r\n                <th id=\"col\"><strong>Qty Transferred</strong></th>\r\n                <th id=\"col\"><strong>Amount Transferred</strong></th>\r\n            </tr>\r\n            <tr id=\"col\">\r\n                {% for row in doc.items%}\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.item_code}}</td>\r\n                <td id=\"col\">{{row.item_name}}</td>\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.tare_weight}}</td>\r\n                <td id=\"col\">{{row.gross_vehicle_weight}}</td>\r\n                <td id=\"col\">{{row.weight_slip_no}}</td>\r\n                <td id=\"col\">{{row.pol_slip_no}}</td>\r\n                <td id=\"col\">{{row.equipment}}</td>\r\n                <td id=\"col\">{{row.equipment_type}}</td>\r\n                <td id=\"col\">{{row.transferred_qty}}</td>\r\n                <td id=\"col\">{{row.amount}}</td>\r\n            </tr>\r\n            {% endfor %}\r\n            <tr id=\"col\">\r\n                <td id=\"col\" colspan=\"11\" style=\"text-align:right;\">\r\n                    <b>Total:</b>\r\n                </td>\r\n                <td id=\"col\">\r\n                    {{doc.total_incoming_value}}\r\n                </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div id=\"in_word\">\r\n        <strong>Amount in Words: </strong> Ngultrum {{ (frappe.utils.money_in_words(doc.total_incoming_value))[3:] }}\r\n    </div>\r\n    <div style=\"padding-top: 150px; clear: both; font-size: small;\">\r\n        <div>Authorized Signatory</div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Issued By) </div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Carrier) </div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Received By) </div>\r\n        <div style=\"padding-top: 80px; text-align:center; clear: both;\">(Verified By) </div>\r\n    </div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
