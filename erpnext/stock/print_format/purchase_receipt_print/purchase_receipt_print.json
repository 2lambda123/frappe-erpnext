 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Purchase Receipt",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">Purchase Receipt Note/ Goods Receipt Note</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">\r\n            <strong>\r\n\t            {{ doc.cost_center }}\r\n            </strong>\r\n        </p>\r\n    </div>\r\n    <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>GRN No :</strong></td>\r\n                <td>{{doc.name}}</td>\r\n                <td><strong>GR Date:</strong></td>\r\n                <td>{{frappe.format_value(doc.posting_date,{\"fieldtype\":\"Date\"})}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>PO No:</strong></td>\r\n                <td>{{doc.items[0].purchase_order}}</td>\r\n                <td><strong>PO Date:</strong></td>\r\n                <td>{{frappe.format_value(frappe.db.get_value(\"Purchase Order\",doc.items[0].purchase_order,\"transaction_date\"),{\"fieldtype\":\"Date\"})}} </td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Supplier/Vendor Name:</strong></td>\r\n                <td>{{doc.supplier}}</td>\r\n                <td><strong>Receiving Warehouse:</strong></td>\r\n                <td>{{ doc.items[0].warehouse}} </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Material Code</strong></th>\r\n                <th id=\"col\"><strong>Material Name</strong></th>\r\n                <th id=\"col\"><strong>UOM</strong></th>\r\n                <th id=\"col\"><strong>QTY</strong></th>\r\n                <th id=\"col\"><strong>Rate</strong></th>\r\n                <th id=\"col\"><strong>Amount Billed</strong></th>\r\n            </tr>\r\n                {% for row in doc.items%}\r\n                    <tr id=\"col\">\r\n                        <td id=\"col\">{{row.idx}}</td>\r\n                        <td id=\"col\">{{row.item_code}}</td>\r\n                        <td id=\"col\">{{row.item_name}}</td>\r\n                        <td id=\"col\">{{row.uom}}</td>\r\n                        <td id=\"col\">{{frappe.format_value(row.qty,{\"fieldtype\":\"Float\"})}}</td>\r\n                        <td id=\"col\">{{frappe.format_value(row.rate,{\"fieldtype\":\"Currency\"}) }}</td>\r\n                        <td id=\"col\">{{ frappe.format_value(row.amount,{\"fieldtype\":\"Currency\"})}} </td>\r\n                    </tr>\r\n            {% endfor %}\r\n            <tr id=\"col\">\r\n                <td id=\"col\" colspan=\"6\" style=\"text-align:right;\">\r\n                    <b>Total:</b>\r\n                </td>\r\n                <td id=\"col\">\r\n                    {{ frappe.format_value(doc.grand_total,{\"fieldtype\":\"Currency\"})}}\r\n                </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div id=\"in_word\">\r\n        <strong>Amount in Words: </strong> Ngultrum {{ (doc.in_words)[3:] }}\r\n    </div>\r\n    <br/>\r\n    <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Authorized Signatory</strong></td>\r\n                <td></td>\r\n            </tr>\r\n        </table>\r\n        <br/>\r\n        <br/>\r\n        <br/>\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Received By</strong> </td>\r\n                <td></td>\r\n                <td><strong>Verified By</strong> </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
