 "html": "{% set debit_entry = doc.entries | get_debit_entry %}\n{% set bank_account = frappe.get_doc(\"Account\", debit_entry.account) %}\n<div class=\"container\" xmlns=\"http://www.w3.org/1999/html\">\n    <div class=\"row clearfix\">\n        <div class=\"col-xs-12 column\">\n            <h3 class=\"text-center\">\n                Pay-in-Slip\n            </h3>\n        </div>\n    </div>\n    <div class=\"row clearfix\">\n        <div class=\"col-xs-6 column\">\n\n            <table class=\"table\">\n                <tr>\n                    <td>\n                        <strong>Bank Name</strong>\n                    </td>\n                    <td>\n                        {{ bank_account.account_name }}\n                    </td>\n                </tr>\n                <tr>\n                    <td>\n                        <strong>Account Number</strong>\n                    </td>\n                    <td>\n                        {{ bank_account.account_number }}\n                    </td>\n                </tr>\n                <tr>\n                    <td>\n                        <strong>Account Holder</strong>\n                    </td>\n                    <td>\n                        {{ bank_account.company }}\n                    </td>\n                </tr>\n            </table>\n\n        </div>\n        <div class=\"col-xs-6 column\">\n\n            <table class=\"table\">\n                <tr>\n                    <td>\n                        <strong>Date</strong>\n                    </td>\n                    <td>\n                        {{ doc.posting_date }}\n                    </td>\n                </tr>\n                <tr>\n                    <td>\n                        Instrument Number\n                    </td>\n                    <td>\n                        {{ doc.cheque_no }}\n                    </td>\n                </tr>\n                <tr>\n                    <td>\n                        Instrument Date\n        