 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">EME Payment Voucher</p>\r\n        <p style=\"font-size:14px;text-align:center\">{{doc.name }}</p>\r\n    </div>\r\n   <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td id =\"less\"><strong>Supplier:</strong></td>\r\n                <td id =\"more\">{{doc.supplier}}</td>\r\n                <td id =\"less\"><strong>Posting Date:</strong></td>\r\n                <td id =\"more\">{{doc.posting_date}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Entry Type</strong></td>\r\n                <td id =\"more\">EME Payment</td>\r\n                <td id =\"less\"><strong>Branch:</strong></td>\r\n                <td id =\"more\">{{doc.branch}}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <P style=\"margin-top:20px; font-size:12px\"><strong>Deduction Details</strong></P>\r\n    <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Account</strong></th>\r\n                <th id=\"col\"><strong>Account Type</strong></th>\r\n                <th id=\"col\"><strong>Amount</strong></th>\r\n                <th id=\"col\"><strong>Remarks</strong></th>\r\n            {% for row in doc.deduct_items%}\r\n            <tr id=\"col\">\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.account}})</td>\r\n                <td id=\"col\">{{row.account_type}}</td>\r\n                <td id=\"col\">{{row.amount}}</td>\r\n                <td id=\"col\">{{row.remarks}}</td>\r\n            </tr>\r\n            {% endfor %}\r\n        </table>\r\n    </div>\r\n    <P style=\"margin-top:20px;font-size:12px\"><strong>Logbook Details</strong></P>\r\n    <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Logbook</strong></th>\r\n                <th id=\"col\"><strong>Equipment</strong></th>\r\n                <th id=\"col\"><strong>Equipment Hiring Form</strong></th>\r\n                <th id=\"col\"><strong>Total Hours</strong></th>\r\n                <th id=\"col\"><strong>Rate</strong></th>\r\n                <th id=\"col\"><strong>Amount</strong></th>\r\n            {% for row in doc.items%}\r\n            <tr id=\"col\">\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.logbook}})</td>\r\n                <td id=\"col\">{{row.equipment}}</td>\r\n                <td id=\"col\">{{row.equipment_hiring_form}}</td>\r\n                <td id=\"col\">{{row.total_hours}}</td>\r\n                <td id=\"col\">{{row.rate}}</td>\r\n                <td id=\"col\">{{row.amount}}</td>\r\n            </tr>\r\n            {% endfor %}\r\n        </table>\r\n    </div>\r\n    \r\n    <div style=\"padding-top: 100px; clear: both;\">\r\n        <div>Authorized Signatory</div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Verified By) </div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Approved By) </div>\r\n    </div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
