 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:20px;text-align:center\">Payment Voucher</p>\r\n    </div>\r\n    <div>\r\n        <p style=\"font-size:18px\">\r\n            {{doc.name}}\r\n        </p>\r\n        <hr>\r\n    </div>\r\n    <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Title:</strong></td>\r\n                <td>{{doc.title}}</td>\r\n                <td><strong>Posting Date:</strong></td>\r\n                <td>{{doc.posting_date}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Payment Type:</strong></td>\r\n                <td>{{doc.payment_type}}</td>\r\n                <td><strong>Branch:</strong></td>\r\n                <td>{{doc.branch}}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n     <div class=\"body_two\">\r\n        <table class =\"table1\">\r\n            <tr id=\"col\">\r\n                <th id=\"col\"><strong>Sl No</strong></th>\r\n                <th id=\"col\"><strong>Reference Doctype</strong></th>\r\n                <th id=\"col\"><strong>Name</strong></th>\r\n                <th id=\"col\"><strong>Grand Total(BTN)</strong></th>\r\n                <th id=\"col\"><strong>Outstanding(BTN)</strong></th>\r\n                <th id=\"col\"><strong>Allocated(BTN)</strong></th>\r\n            </tr>\r\n            {% for row in doc.references%}\r\n            <tr id=\"col\">\r\n                <td id=\"col\">{{row.idx}}</td>\r\n                <td id=\"col\">{{row.reference_doctype}}</td>\r\n                <td id=\"col\">{{row.reference_name}}</td>\r\n                <td id=\"col\">{{row.total_amount}}</td>\r\n                <td id=\"col\">{{row.outstanding_amount}}</td>\r\n                <td id=\"col\">{{row.allocated_amount}}</td>\r\n            </tr>\r\n            {% endfor %}\r\n        </table>\r\n    </div>\r\n    \r\n    <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Party Type:</strong></td>\r\n                <td>{{doc.party_type}}</td>\r\n                <td><strong>Party:</strong></td>\r\n                <td>{{doc.party}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Account Paid From:</strong></td>\r\n                <td>{{doc.paid_from}}</td>\r\n                <td><strong>Account Paid To:</strong></td>\r\n                <td>{{doc.paid_to}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td><strong>Account Currency:</strong></td>\r\n                <td>{{doc.paid_from_account_currency}}</td>\r\n                <td><strong>Paid Amount (BTN):</strong></td>\r\n                <td>{{doc.paid_amount}}</td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div class=\"row section-break\" style=\"margin-top: 60px;\">\r\n\t\t<div class=\"col-xs-6 column-break\" style=\"text-align: left;\">Prepared By</div>\r\n\t\t<div class=\"col-xs-6 column-break\" style=\"text-align: center;\">Verified By</div>\r\n\t</div>\r\n\t<div class=\"row section-break\" style=\"margin-top: 60px;\">\r\n\t\t<div class=\"col-xs-6 column-break\" style=\"text-align: left;\">Approved By</div>\r\n\t\t<div class=\"col-xs-6 column-break\" style=\"text-align: center;\">Received By</div>\r\n\t\t<br><br>\r\n\t\t<hr>\r\n\t</div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
