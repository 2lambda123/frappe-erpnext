 "html": "<div class=\"container\">\r\r{% if letter_head and not no_letterhead %}\r    <p class=\"text-left\">\r\r    <div class=\"letter-head\">{{ letter_head }}</div>\r    <div class=\"print-heading\">\r        <h3>{{ doc.select_print_heading or (doc.print_heading if doc.print_heading != None\r\t\t\telse _(doc.doctype)) }}</h3>\r    </div>\r\r    </p>\r    <br/>\r    <br/>\r{% endif %}\r\r\r<div class=\"row clearfix\">\r    <div class=\"col-xs-12 column\">\r\r        <table class=\"table table-condensed\">\r\r            <thead>\r            <tr>\r                <th>\r                    Date\r                </th>\r                <th>\r                    LR Number\r                </th>\r            </tr>\r            </thead>\r\r\r            <tbody>\r            <tr>\r                <td>{{ doc.get_formatted(\"posting_date\") }}</td>\r                <td>{{ doc.get_formatted(\"name\") }}</td>\r            </tr>\r            </tbody>\r\r        </table>\r\r    </div>\r</div>\r\r<div class=\"row clearfix\">\r\r    <div class=\"col-xs-6 column\">\r        <p class=\"text-left\">\r            <strong>CONSIGNEE</strong>\r\r        <div>\r            {{ doc.customer }}\r            {% if doc.address_display %}\r                <br/>{{ doc.get_formatted(\"address_display\") }}\r            {% endif %}\r        </div>\r\r        <div>\r            {% set customer =  frappe.get_doc(\"Customer\", doc.customer) %}\r            {% if customer.tin_number %}\r                TIN No. {{ customer.tin_number }}\r            {% endif %}\r        </div>\r        </p>\r\r\r    </div>\r\r    <div class=\"col-xs-6 column\">\r        <p class=\"text-left\">\r            <strong>CONSIGNOR</strong>\r        </p>\r\r        <div>\r            {{ doc.consignor }}\r            {% set consignor_addr = doc.consignor | get_address_display('Billing') %}\r            {% if consignor_addr %}\r                <br/>{{ consignor_addr }}\r            {% endif %}\r            <div/>\r\r            <div>\r                {% set consignor = frappe.get_doc(\"Supplier\", doc.consignor) %}\r                {% if consignor.tin_number %}\r                    TIN No. {{ consignor.tin_number }}\r                {% endif %}\r            </div>\r\r        </div>\r\r    </div>\r\r</div>\r\r\r<div class=\"row clearfix\">\r    <div class=\"col-xs-12 column\">\r        <table class=\"table table-condensed\">\r\r            <thead>\r            <tr>\r                <th>\r                    #\r                </th>\r                <th>\r                    Particulars\r                </th>\r                <th>\r                    Qty\r                </th>\r                <th>\r                    Rate\r                </th>\r                <th>\r                    Charges\r                </th>\r            </tr>\r            </thead>\r\r            <tbody>\r            {% for row in doc.entries %}\r                <tr>\r                    <td>\r                        {{ row.idx }}\r                    </td>\r                    <td width=\"70%\">\r                        <strong>{{ row.get_formatted(\"item_name\") }}</strong><br/>\r                        {{ row.get_formatted(\"description\") }}\r                    </td>\r                    <td>\r                        {{ row.get_formatted(\"qty\") }}\r                    </td>\r                    <td>\r                        {{ row.get_formatted(\"rate\") }}\r                    </td>\r                    <td>\r                        {{ row.get_formatted(\"amount\") }}\r                    </td>\r                </tr>\r            {% endfor %}\r            </tbody>\r\r        </table>\r\r    </div>\r</div>\r\r\r<div class=\"row clearfix\">\r\r    <div class=\"col-xs-6 column\">\r        <p class=\"text-left\">\r        </p>\r\r        <div>\r            {{ doc.get_formatted(\"terms\") }}\r        </div>\r\r    </div>\r\r    <div class=\"col-xs-6 column\">\r\r        <table class=\"table table-condensed\">\r            <tbody>\r\r            <tr class=\"active\">\r                <td width=\"70%\">\r                    <strong>Net Total</strong>\r                </td>\r                <td>\r                    {{ doc.get_formatted(\"net_total_export\") }}\r                </td>\r            </tr>\r\r            {% for row in doc.other_charges %}\r                <tr>\r                    <td width=\"70%\">\r                        <strong>{{ row.get_formatted(\"description\") }} @ {{ row.get_formatted(\"rate\") }}%</strong>\r                    </td>\r                    <td>\r                        {{ row.get_formatted(\"tax_amount\") }}\r                    </td>\r                </tr>\r            {% endfor %}\r\r            <tr class=\"active\">\r                <td width=\"70%\">\r                    <strong>Grand Total</strong>\r                </td>\r                <td>\r                    {{ doc.get_formatted(\"grand_total_export\") }}\r                </td>\r            </tr>\r\r            </tbody>\r\r        </table>\r\r    </div>\r\r</div>\r\r\r<div class=\"row clearfix\">\r    <div class=\"col-xs-12 column\">\r\r        <p class=\"text-right\">\r            For Arun Logistics\r            </br>\r            </br>\r            Authorised Signatory\r        </p>\r    </div>\r</div>\r\r</div>", 
