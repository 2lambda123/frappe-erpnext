 "html": "<!DOCTYPE html>\r\n<head>\r\n\t<div >\r\n        <div class=\"letter-head\">\r\n                {{letter_head}}\r\n        </div>\r\n    </div>\r\n</head>\r\n\r\n<body>\r\n    <div class=\"body_one\">\r\n        <p style=\"font-weight:bold;font-size:18px;text-align:center\">Payment Voucher</p>\r\n        <p style=\"font-size:14px;text-align:center;text-decoration:underline\">{{ doc.branch }}</p>\r\n    </div>\r\n   \r\n   <div id=\"div_tab\">\r\n        <table id=\"div_tab\">\r\n            <tr>\r\n                <td><strong>Company:</strong></td>\r\n                <td>{{doc.company}}</td>\r\n                <td><strong>Posting Date:</strong></td>\r\n                <td>{{doc.posting_date}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Equipment:</strong></td>\r\n                <td id =\"more\">{{doc.equipment}}</td>\r\n                <td id =\"less\"><strong>Equipment Owner</strong></td>\r\n                <td id =\"mo\">{{doc.supplier}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Total No.of Trips:</strong></td>\r\n                <td id =\"more\">{{doc.total_trip}}</td>\r\n                <td id =\"less\"><strong>Gross Amount</strong></td>\r\n                <td id =\"mo\">{{doc.gross_amount}}</td>\r\n            </tr>\r\n            {% if doc.deductions %}\r\n            <tr>\r\n                <td id =\"less\" colspan = \"4\"><strong>Deductions:</strong></td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Clearing Amount:</strong></td>\r\n                <td id =\"more\">{{doc.clearing_amount}}</td>\r\n                <td id =\"less\"><strong>Security Deposit</strong></td>\r\n                <td id =\"mo\">{{doc.security_deposit_amount}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Weighbridge Amount:</strong></td>\r\n                <td id =\"more\">{{doc.weighbridge_amount}}</td>\r\n                <td id =\"less\"><strong>TDS Amount</strong></td>\r\n                <td id =\"mo\">{{doc.tds_amount}}</td>\r\n            </tr>\r\n            <tr>\r\n                <td id =\"less\"><strong>Total Amount Payable:</strong></td>\r\n                <td id =\"mo\" colspan = \"3\">{{doc.amount_payable}}</td>\r\n            </tr>\r\n            {% endif %}\r\n        </table>\r\n    </div>\r\n    \r\n    <div style=\"padding-top: 30px; clear: both;\">\r\n        <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Prepared By) </div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Verified By) </div>\r\n    </div>\r\n    <div style=\"padding-top: 50px; clear: both;\">\r\n        <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Approved By) </div>\r\n        <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Received  By) </div><br/>\r\n        <hr style=\"width:80%;float:center\">\r\n    </div>\r\n    <div class=\"footer\">\r\n        {% if not no_letterhead and footer %}\r\n        <div class=\"letter-head-footer\">\r\n          {{ footer }}\r\n        </div>\r\n        {% endif %}\r\n        <p class=\"text-center small page-number visible-pdf\">\r\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\r\n        </p>\r\n    </div>\r\n</body>",
